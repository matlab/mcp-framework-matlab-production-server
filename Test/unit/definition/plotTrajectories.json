{
  "tools": [{
    "name": "plotTrajectories",
    "description": "Generate a 3x3 plot of trajectories from 3-axis seismometer data. Plot each axis against each other. On the diagonal place histograms of the data from each axis.",
    "inputSchema": {
      "type": "object",
      "properties": {
        "quakeURL": {
          "type": "string",
          "description": "file: URL containing a table of uncorrected 3-axis seismometer accelerations"
        },
        "sampleRate": {
          "type": "number",
          "description": "Seismometer sample rate in Hertz"
        },
        "correction": {
          "type": "number",
          "description": "Instrument correction: apply to quakeData"
        },
        "start": {
          "type": "number",
          "description": "Start offset, in seconds, for trajectory data"
        },
        "stop": {
          "type": "number",
          "description": "Stop offset, in seconds, for trajectory data"
        },
        "plotURL": {
          "type": "string",
          "description": "file: URL to save the plot into."
        }
      },
      "required": [
        "quakeURL",
        "sampleRate",
        "correction",
        "start",
        "stop",
        "plotURL"
      ]
    },
    "outputSchema": {
      "type": "object",
      "properties": {
        "status": {
          "type": "boolean",
          "description": "True on success, false on error or failure."
        },
        "msg": {
          "type": "string",
          "description": "Non-empty error message if status is false."
        }
      },
      "required": []
    }
  }],
  "signatures": [{
    "plotTrajectories": {
      "function": "plotTrajectoriesMCP",
      "input": {
        "name": [
          "quakeURL",
          "sampleRate",
          "correction",
          "start",
          "stop",
          "plotURL"
        ],
        "type": [
          "string",
          "double",
          "double",
          "double",
          "double",
          "string"
        ]
      },
      "output": {
        "name": [
          "status",
          "msg"
        ],
        "type": [
          "logical",
          "string"
        ]
      }
    }
  }]
}
